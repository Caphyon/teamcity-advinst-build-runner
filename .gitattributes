# Auto detect text files and perform LF normalization
* text=auto

/.git* text eol=lf
*.sh   text eol=lf

.editorconfig text eol=lf

# Web
.htaccess text eol=lf
*.css  text eol=lf diff=css
*.htm  text eol=lf diff=html
*.html text eol=lf diff=html
*.js   text eol=lf
*.md   text eol=lf
*.php  text eol=lf
*.py   text eol=lf
*.resx text eol=lf
*.xml  text eol=lf
*.xsd  text eol=lf
*.xsl  text eol=lf
*.json text eol=lf
*.java text eol=lf
*.xml  text eol=lf
*.jsp  text eol=lf


# LFS
# =========================

# Binaries & Libs
*.app  filter=lfs diff=lfs merge=lfs -text
*.bin  filter=lfs diff=lfs merge=lfs -text
*.cer  filter=lfs diff=lfs merge=lfs -text
*.chm  filter=lfs diff=lfs merge=lfs -text
*.com  filter=lfs diff=lfs merge=lfs -text
*.dat  filter=lfs diff=lfs merge=lfs -text
*.dll  filter=lfs diff=lfs merge=lfs -text
*.eap  filter=lfs diff=lfs merge=lfs -text
*.exe  filter=lfs diff=lfs merge=lfs -text
*.ima  filter=lfs diff=lfs merge=lfs -text
*.lib  filter=lfs diff=lfs merge=lfs -text
*.msi  filter=lfs diff=lfs merge=lfs -text
*.msm  filter=lfs diff=lfs merge=lfs -text
*.msp  filter=lfs diff=lfs merge=lfs -text
*.obj  filter=lfs diff=lfs merge=lfs -text
*.out  filter=lfs diff=lfs merge=lfs -text
*.pcp  filter=lfs diff=lfs merge=lfs -text
*.pfx  filter=lfs diff=lfs merge=lfs -text
*.pvk  filter=lfs diff=lfs merge=lfs -text
*.vmdk filter=lfs diff=lfs merge=lfs -text
*.vsix filter=lfs diff=lfs merge=lfs -text

